<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.bank_applicaiton">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Bank_Applicaiton">
        <activity android:name=".PatchEmployeeActivity">
            <intent-filter>
                <action android:name="com.example.myapplication.PatchEmployeeActivity" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".ChooseEmployeeActivity">
            <intent-filter>
                <action android:name="com.example.myapplication.ChooseEmployeeActivity" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".EmployeeActivity">
            <intent-filter>
                <action android:name="com.example.myapplication.EmployeeActivity" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".EmployeeActivitySing">
            <intent-filter>
                <action android:name="com.example.myapplication.EmployeeActivitySing" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".PostApplicationClientActivity">
            <intent-filter>
                <action android:name="com.example.myapplication.PostApplicationClientActivity" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".PostApplicationActivity">
            <intent-filter>
                <action android:name="com.example.myapplication.PostApplicationActivity" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".GetApplicationActivity">
            <intent-filter>
                <action android:name="com.example.myapplication.GetApplicationActivity" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".ClientActivity">
            <intent-filter>
                <action android:name="com.example.myapplication.ClientActivity" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".Sing_Activity">
            <intent-filter>
                <action android:name="com.example.myapplication.Sing_Activity" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>